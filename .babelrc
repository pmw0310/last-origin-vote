{
    "env": {
        "development": {
            "presets": ["next/babel"],
            "plugins": [["styled-components", { "ssr": true, "fileName": true, "displayName": true, "pure": true, "preprocess": false }]]
        },
        "production": {
            "presets": ["next/babel"],
            "plugins": [["styled-components", { "ssr": true, "fileName": true, "displayName": true, "pure": true, "preprocess": false }]]
        },
        "test": {
            "presets": [
                ["next/babel", { "preset-env": { "modules": "commonjs" } }]
            ],
            "plugins": [["styled-components", { "ssr": true, "fileName": true, "displayName": true, "pure": true, "preprocess": false }]]
        }
    }
}
